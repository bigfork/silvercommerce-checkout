{
    "name": "silvercommerce/checkout",
    "description": "Adds a 'checkout' to a SilverStripe site which links to an Estimate and adds the ability to setup and pay",
      "type": "silverstripe-vendormodule",
      "homepage": "http://github.com/silvercommerce/checkout",
      "keywords": [
          "silverstripe",
          "commerce",
          "ecommerce",
          "checkout",
          "payments"
      ],
      "license": "BSD-3-Clause",
      "authors": [
          {
              "name": "Morven Lewis-Everley",
              "email": "morven@ilateral.co.uk"
          },
          {
              "name": "Mark Anderson",
              "email": "mark@ilateral.co.uk"
          }
      ],
      "support": {
          "issues": "http://github.com/silvercommerce/checkout/issues"
      },
      "require": {
          "silverstripe/framework": "^5",
          "silverstripe/silverstripe-omnipay": "^4",
          "silvercommerce/settings": "dev-ss5",
          "silvercommerce/orders-admin": "dev-ss5",
          "silvercommerce/contact-admin": "dev-ss5",
          "silvercommerce/postage": "dev-ss5"
      },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-esttings"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-orders-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-contact-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-postage"
        }
    ],
      "suggest": {
          "silverstripe/cms": "Add full content management support to your site",
          "i-lateral/silverstripe-users": "Add user account management and registration"
      },
      "extra": {
          "expose": [
              "client/dist"
          ]
      },
      "scripts": {
          "lint": "phpcs -s src/ tests/",
          "lint-clean": "phpcbf src/ tests/"
      },
      "autoload": {
          "psr-4": {
              "SilverCommerce\\Checkout\\": "src/"
          }
      }
  }
